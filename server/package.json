		"pg": "^8.9.0",
		"swc": "^1.0.11",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.5"
	},
	"devDependencies": {
		"@types/express": "^4.17.17",
		"@types/express-list-endpoints": "^6.0.0",
