{
  "name": "ai-news-outlet-api",
  "version": "0.1.0",
  "description": "API for an AI driven news application",
  "main": "app.ts",
  "scripts": {
    "start": "ts-node app.ts",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register app.ts",
    "build": "swc app.ts -d dist",
    "typecheck": "tsc --noEmit --listFiles --pretty",
    "assure": "echo \"No tests configured!\"",
    "pretty": "prettier --write .",
    "lint": "eslint"
  },
  "author": "BIT Third Years",
  "license": "ISC",
  "dependencies": {
    "ai-daily": "^1.0.3",
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "eslint": "^8.35.0",
    "express": "^4.18.2",
    "nodemon": "^2.0.20",
    "pg": "^8.9.0",
    "pg-format": "^1.0.4",
    "prettier": "^2.8.4",
    "swc": "^1.0.11",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-list-endpoints": "^6.0.0",
    "@types/pg": "^8.6.6",
    "@types/pg-format": "^1.0.2",
    "tsconfig-paths": "^4.1.2"
  }
}
